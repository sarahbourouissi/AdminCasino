<VBox fx:id="sidePane" prefHeight="700.0" prefWidth="200.0" style="-fx-background-color: #3B0405;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AdminDrawerController">
